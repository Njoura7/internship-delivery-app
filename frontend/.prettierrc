{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/hooks",
    "^@/contexts",
    "^@/providers",
    "",
    "^@/apis",
    "^@/services",
    "",
    "^@mui",
    "",
    "^@/pages",
    "",
    "^@/components",
    "^\\./",
    "",
    "^@/utils",
    "^@/enums",
    "^@/theme"
  ]
}
